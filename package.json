{
  "name": "dashboard",
  "version": "0.0.1",
  "description": "Docker management",
  "main": "index.js",
  "scripts": {
    "format": "./node_modules/.bin/prettier --write \"./app/**/*.{js,jsx,less}\"",
    "stylelint": "./node_modules/.bin/stylelint \"app/**/*.{css,less}\"",
    "eslint": "./node_modules/.bin/eslint --fix --ext .jsx \"./app/**/*.{js,jsx}\"",
    "lint": "npm run eslint && npm run stylelint",
    "doc": "./node_modules/.bin/esdoc",
    "prestart": "mkdir -p ./dist && cp ./app/index.html ./dist/",
    "start": "API_URL=https://dashboard-api.vibioh.fr WS_URL=wss://dashboard-api.vibioh.fr/ws AUTH_URL=https://dashboard-auth.vibioh.fr BASIC_AUTH_ENABLED=false GITHUB_AUTH_ENABLED=true ./node_modules/.bin/webpack-dev-server --hot --inline --content-base dist --history-api-fallback --output-public-path static",
    "test": "./node_modules/.bin/nyc --reporter text --reporter lcov --extension .jsx --require babel-core/register ./node_modules/.bin/ava",
    "test:e2e": "./node_modules/.bin/codeceptjs run-multiple --all",
    "copy:web": "cp ./web/*.txt ./dist/ && cp ./web/favicon/* ./dist/",
    "mustache": "./node_modules/js-index-builder/index.js --template \"./app/*.html\" --partials \"./web/partials/*.html\" --bust `git log --pretty=format:'%h' -n 1` --output ./tmp/",
    "html-minify": "./node_modules/.bin/html-minifier --config-file ./html-minifier.json --input-dir ./tmp --output-dir ./dist",
    "web": "npm run copy:web && npm run mustache && npm run html-minify",
    "prebuild": "mkdir -p ./dist",
    "build": "npm run format && npm run lint && npm test && npm run doc && npm run web && ./node_modules/.bin/webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/dashboard.git"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-dom",
    "react-router-redux",
    "history",
    "redux",
    "redux-saga",
    "docker",
    "nyc",
    "eslint",
    "webpack",
    "babel"
  ],
  "author": "Vincent Boutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/dashboard/issues"
  },
  "homepage": "https://github.com/ViBiOh/dashboard",
  "devDependencies": {
    "ava": "latest",
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-plugin-transform-object-rest-spread": "latest",
    "babel-preset-env": "latest",
    "babel-preset-react": "latest",
    "codeceptjs-webdriverio": "latest",
    "css-loader": "latest",
    "enzyme": "latest",
    "enzyme-adapter-react-16": "latest",
    "esdoc": "latest",
    "esdoc-coverage-plugin": "latest",
    "esdoc-ecmascript-proposal-plugin": "latest",
    "esdoc-jsx-plugin": "latest",
    "esdoc-publish-html-plugin": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-loader": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "html-minifier": "latest",
    "identity-obj-proxy": "latest",
    "ignore-styles": "latest",
    "js-index-builder": "ViBiOh/js-index-builder",
    "jsdom": "latest",
    "less": "latest",
    "less-loader": "latest",
    "normalize.css": "latest",
    "nyc": "latest",
    "prettier": "latest",
    "react-addons-test-utils": "latest",
    "react-test-renderer": "latest",
    "sinon": "latest",
    "style-loader": "latest",
    "stylelint": "latest",
    "stylelint-config-standard": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest"
  },
  "dependencies": {
    "babel-polyfill": "latest",
    "chart.js": "latest",
    "classnames": "latest",
    "express": "latest",
    "funtch": "latest",
    "history": "latest",
    "moment": "latest",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-icons": "latest",
    "react-redux": "latest",
    "react-router": "latest",
    "react-router-dom": "latest",
    "react-router-redux": "latest",
    "redux": "latest",
    "redux-saga": "latest"
  },
  "ava": {
    "files": [
      "app/**/*.test.js"
    ],
    "require": [
      "ignore-styles",
      "./app/utils/Test.js"
    ],
    "concurrency": 4,
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1%"
            ]
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "esdoc": {
    "source": "./app",
    "includes": [
      ".(js|jsx)$"
    ],
    "excludes": [
      ".test\\.(js|jsx)$"
    ],
    "destination": "./doc",
    "plugins": [
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "classProperties": true,
          "objectRestSpread": true,
          "decorators": true,
          "doExpressions": true,
          "functionBind": true,
          "asyncGenerators": true,
          "exportExtensions": true,
          "dynamicImport": true
        }
      },
      {
        "name": "esdoc-jsx-plugin",
        "option": {
          "enable": true
        }
      },
      {
        "name": "esdoc-publish-html-plugin"
      },
      {
        "name": "esdoc-coverage-plugin",
        "option": {
          "enable": true
        }
      }
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": 1,
      "react/jsx-closing-tag-location": 1,
      "react/jsx-indent": 1
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "declaration-colon-newline-after": null,
      "selector-list-comma-newline-after": null,
      "indentation": null
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
