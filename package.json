{
  "name": "dashboard",
  "version": "0.0.1",
  "description": "Docker management",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"./src/**/*.{js,jsx,css}\"",
    "stylelint": "stylelint \"./src/**/*.{css}\"",
    "eslint": "eslint --fix --ext .jsx \"./src/**/*.{js,jsx}\"",
    "lint": "npm run eslint && npm run stylelint",
    "doc": "esdoc",
    "start": "NODE_ENV=staging parcel src/index.html --public-url / -d ./dist --open",
    "test": "nyc --reporter text --reporter lcov --extension .jsx --require @babel/register ava --verbose",
    "prebuild": "npm run format && npm run lint && npm test",
    "build": "parcel build src/index.html --public-url / -d ./dist",
    "postbuild": "cp ./src/robots.txt ./src/favicon/* ./dist/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/dashboard.git"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-dom",
    "react-router-redux",
    "history",
    "redux",
    "redux-saga",
    "docker",
    "nyc",
    "eslint",
    "parcel-bundler",
    "babel"
  ],
  "author": "Vincent Boutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/dashboard/issues"
  },
  "homepage": "https://github.com/ViBiOh/dashboard",
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "ava": "1.0.0-rc.1",
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "esdoc": "1.1.0",
    "esdoc-coverage-plugin": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-jsx-plugin": "1.0.0",
    "esdoc-publish-html-plugin": "1.1.2",
    "eslint": "5.8.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "identity-obj-proxy": "3.0.0",
    "ignore-styles": "5.0.1",
    "jsdom": "13.0.0",
    "nyc": "13.1.0",
    "parcel-bundler": "1.10.3",
    "postcss-modules": "1.4.1",
    "prettier": "1.14.3",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.6.0",
    "sinon": "7.1.1",
    "stylelint": "9.7.1",
    "stylelint-config-standard": "18.2.0"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "chart.js": "2.7.3",
    "classnames": "2.2.6",
    "express": "4.16.4",
    "funtch": "1.5.7",
    "history": "4.7.2",
    "moment": "2.22.2",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-icons": "3.2.2",
    "react-redux": "5.1.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "redux": "4.0.1",
    "redux-saga": "0.16.2"
  },
  "ava": {
    "files": [
      "./src/**/*.test.js"
    ],
    "require": [
      "@babel/register",
      "ignore-styles",
      "./src/utils/Test.js"
    ]
  }
}
