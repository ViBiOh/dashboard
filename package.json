{
  "name": "docker-deploy",
  "version": "0.0.1",
  "description": "Docker management",
  "main": "index.js",
  "scripts": {
    "copy:html": "mkdir -p ./dist && cp ./app/index.html ./dist/",
    "copy:web": "cp ./web/*.txt ./web/*.html ./dist/ && cp ./web/favicon/* ./dist/",
    "mustache": "./mustache.js -t \"./app/*.html\" -b `git log --pretty=format:'%h' -n 1` -o ./dist/",
    "eslint": "./node_modules/.bin/eslint --ext .js --ext .jsx app/",
    "sassLint": "./node_modules/.bin/sass-lint \"app/**/*.css\" -v -s scss",
    "lint": "npm run eslint && npm run sassLint",
    "start": "npm run copy:html && ./node_modules/.bin/webpack-dev-server --hot --inline --content-base dist --history-api-fallback --output-public-path static",
    "test": "./node_modules/.bin/nyc --reporter html --reporter text --exclude **/*.test.js* --extension .jsx --require babel-core/register ./node_modules/.bin/mocha --require ./test/setup.js --require ignore-styles \"app/**/*.test.js*\" --",
    "build": "npm run lint && npm test && npm run mustache && npm run copy:web && ./node_modules/.bin/webpack --progress -p"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/docker-deploy.git"
  },
  "keywords": [
    "react",
    "react-router",
    "redux",
    "redux-saga",
    "docker"
  ],
  "author": "Vincent Boutour",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ViBiOh/docker-deploy/issues"
  },
  "homepage": "https://github.com/ViBiOh/docker-deploy",
  "devDependencies": {
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-plugin-transform-object-rest-spread": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "chai": "latest",
    "css-loader": "latest",
    "enzyme": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-loader": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "extract-text-webpack-plugin": "latest",
    "glob": "latest",
    "identity-obj-proxy": "latest",
    "ignore-styles": "latest",
    "js-utils": "git+https://github.com/ViBiOh/js-utils.git",
    "jsdom": "latest",
    "mkdirp": "latest",
    "mocha": "latest",
    "mustache": "latest",
    "node-sass": "latest",
    "normalize.css": "latest",
    "nyc": "latest",
    "react-addons-test-utils": "latest",
    "sass-lint": "latest",
    "sass-loader": "latest",
    "sinon": "latest",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-dev-server": "latest",
    "yargs": "latest"
  },
  "dependencies": {
    "babel-polyfill": "latest",
    "classnames": "latest",
    "js-fetch": "git+https://github.com/ViBiOh/js-fetch.git",
    "moment": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-icons": "latest",
    "react-redux": "latest",
    "react-router": "latest",
    "react-router-redux": "latest",
    "redux": "latest",
    "redux-saga": "latest"
  }
}
