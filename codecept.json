      "protocol": "http",
      "host": "localhost",
      "port": "4444",
      "url": "https://front:1080",
      "browser": "chrome",
      "desiredCapabilities": {
        "javascriptEnabled": true,
        "webStorageEnabled": true,
        "acceptSslCerts": true
      }
    }
  },
  "include": {
    "loginPage": "./e2e/pages/Login.js",
    "listPage": "./e2e/pages/List.js"
  },
  "mocha": {},
  "multiple": {
